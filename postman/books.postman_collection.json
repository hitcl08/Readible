							"var imageLink =   '\"'+jsonData.items[0].volumeInfo.imageLinks.thumbnail+'\"';\r",
							"var rating = Math.floor(Math.random() * 5) + 1 ;\r",
							"\r",
							"postman.setGlobalVariable(\"title\", title);\r",
							"postman.setGlobalVariable(\"author\", author);\r",
							"postman.setGlobalVariable(\"description\", description);\r",
							"postman.setGlobalVariable(\"imageLink\", imageLink);\r",
							"postman.setGlobalVariable(\"rating\", rating);\r",
							"\r",
							"postman.setNextRequest(\"add_books\");\r",
							"\r",
							""
